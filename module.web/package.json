{
  "name": "era-lanaguagetroubleshooter",
  "version": "0.2.0-alpha.1",
  "private": true,
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/era-lanaguagetroubleshooter/era-lanaguagetroubleshooter.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "eslint --fix --ext .ts --ext .tsx --ext .js ./src && stencil build --docs",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@eraware/dnn-elements": "^0.9.1",
    "@stencil/core": "1.17.3",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.30.1"
  },
  "license": "MIT"
}
